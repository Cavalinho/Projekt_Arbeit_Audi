                  <% @uebergabe_KW_aktuell.each do |product| %>
                 <li> Vorhaben: <%=product.Vorhaben%> Statusdatum: <%=product.P_Statusdatum%> SET: <%=product.SET%> Beschreibung: <%=product.Denominacion%> Sachbearbeiter: <%=product.Sachbearbeiter%></li><       >
